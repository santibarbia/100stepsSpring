import org.springframework.web.bind.annotation.RestController;



@RestController
public class LoginController {
	
	@GetMapping("/login")
	public String loginMessage() {
		//model.put("name", name);
		return "login";
	}
	
	
	@PostMapping("/login")
	public String login() {
		//model.put("name", name);
		return "welcome";
	}
	
}
